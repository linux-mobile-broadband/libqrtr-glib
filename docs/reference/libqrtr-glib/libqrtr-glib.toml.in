[library]
version = "@VERSION@"
description = "A glib-based library for communicating with services provided by remote processors using Qualcomm's IPC Router protocol QRTR "
license = "LGPL-2.1-or-later"
authors = "The libqrtr authors"
browse_url = "https://gitlab.freedesktop.org/mobile-broadband/libqrtr.git"
repository_url = "https://gitlab.freedesktop.org/mobile-broadband/libqrtr/"
website_url = "https://gitlab.freedesktop.org/mobile-broadband/libqrtr/"
dependencies = [ "GObject-2.0", "Gio-2.0"]
devhelp = true
search_index = true

  [dependencies."GObject-2.0"]
  name = "GObject"
  description = "The base type system library"
  docs_url = "https://docs.gtk.org/gobject/"

  [dependencies."Gio-2.0"]
  name = "Gio"
  description = "GObject Interfaces and Objects, Networking, IPC, and I/O"
  docs_url = "https://docs.gtk.org/gio/"

[theme]
name = "basic"
show_index_summary = true
show_class_hierarchy = true

[extra]
urlmap_file = "urlmap.js"

[source-location]
# The base URL for the web UI
base_url = "https://gitlab.freedesktop.org/mobile-broadband/libqrtr/-/blob/main/"
# The format for links, using "filename" and "line" for the format
file_format = "{filename}#L{line}"
